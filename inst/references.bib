  author = {Su, Yin and Rao, Li-Lin and Sun, Hong-Yue and Du, Xue-Lei and Li, Xingshan and Li, Shu},
  date = {2013},
  journaltitle = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
