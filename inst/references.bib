@article{solway2019,
  title = {Loss {{Aversion Correlates With}} the {{Propensity}} to {{Deploy Model}}-{{Based Control}}},
  author = {Solway, Alec and Lohrenz, Terry and Montague, P. Read},
  date = {2019-09-06},
  journaltitle = {Front. Neurosci.},
  volume = {13},
